<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#68070000"
    tools:context=".AddTaxCategory.AddTaxCategoryActivity">

    <androidx.cardview.widget.CardView
        android:layout_width="0dp"
        android:layout_height="0dp"

        app:cardCornerRadius="8dp"
        app:cardBackgroundColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.70"
        app:layout_constraintHeight_percent="0.65"
        >
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="20dp">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto_bold"
            android:text="company Branch"
            android:textAllCaps="true"
            android:textColor="@color/red"
            android:textSize="20sp"
            app:autoSizeMaxTextSize="20sp"
            app:autoSizeMinTextSize="16sp"
            app:autoSizeStepGranularity="1sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <ImageView
            android:id="@+id/close"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/ic_close"
            app:layout_constraintBottom_toBottomOf="@+id/textView2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:tint="@color/appMainColoro" />
        <View
            android:id="@+id/view9"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginTop="20dp"
            android:background="@color/layer"
            app:layout_constraintTop_toBottomOf="@+id/textView2"/>



        <TextView
            android:id="@+id/textView35"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="12dp"
            android:layout_marginTop="18dp"
            android:background="@drawable/gradinat_background"
            android:elevation="12dp"
            android:fontFamily="@font/roboto_bold"
            android:padding="5dp"
            android:text="Tax Category Name"
            android:textColor="@color/appMainColor"
            android:textSize="13sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view9" />


        <androidx.cardview.widget.CardView
            android:id="@+id/con10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
     app:cardCornerRadius="10dp"
            app:cardElevation="0dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/view9"
            app:layout_constraintWidth_percent="0.45"


            >

            <androidx.constraintlayout.widget.ConstraintLayout

                android:background="@drawable/card_with_border_main2"
                android:layout_width="match_parent"
                android:layout_height="51dp"

                >


                <EditText
                    android:id="@+id/taxName"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:textSize="13sp"
                    android:padding="10dp"
                    android:background="@drawable/back"
                    android:textColorHint="@color/border"
                    android:fontFamily="@font/roboto_bold"
                    android:textColor="@color/appMainColor" />



            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>

        <androidx.cardview.widget.CardView
            android:id="@+id/cardView15"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_margin="20dp"
            android:elevation="0dp"
            app:cardCornerRadius="7dp"
            app:layout_constraintHeight_percent="0.45"
            app:layout_constraintTop_toBottomOf="@+id/con10"
           >

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:background="@color/back"
                android:padding="10dp">

                <TextView
                    android:id="@+id/textView3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Tax Rates"
                    android:textAllCaps="true"
                    android:textColor="@color/appMainColor"
                    android:textSize="20sp"
                    android:textStyle="bold"
                    app:autoSizeMaxTextSize="20sp"
                    app:autoSizeMinTextSize="17sp"
                    app:autoSizeStepGranularity="1sp"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:layout_margin="20dp"
                    android:elevation="0dp"
                    app:cardCornerRadius="7dp"
                    app:layout_constraintHeight_percent="0.70"
                    app:layout_constraintTop_toBottomOf="@+id/textView3"
                    tools:layout_editor_absoluteX="30dp">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/white"
                        android:padding="10dp">


                        <TextView
                            android:id="@+id/textView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="12dp"
                            android:layout_marginTop="18dp"
                            android:background="@drawable/gradinat_background"
                            android:elevation="12dp"
                            android:fontFamily="@font/roboto_bold"
                            android:padding="5dp"
                            android:text="country"
                            android:textColor="@color/appMainColor"
                            android:textSize="13sp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent" />


                        <androidx.cardview.widget.CardView
                            android:id="@+id/con"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="32dp"
                            app:cardCornerRadius="12dp"
                            app:cardElevation="0dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:layout_constraintWidth_percent="0.32"


                            >

                            <androidx.constraintlayout.widget.ConstraintLayout

                                android:layout_width="match_parent"
                                android:layout_height="51dp"
                                android:background="@drawable/card_with_border_main2"

                                >



                                <Spinner
                                    android:id="@+id/countrySpinner"
                                    android:layout_width="match_parent"
                                    android:padding="5dp"
                                    android:layout_height="match_parent"
                                    android:autofillHints="Drinks Category"
                                    android:contentDescription="@string/app_name"
                                    android:dropDownSelector="@color/appMainColor"
                                    android:overlapAnchor="false"
                                    android:popupBackground="@color/white"
                                    android:spinnerMode="dropdown"
                                    android:visibility="visible"
                                    style="@style/SpinnerTheme"
                                    android:gravity="start"
                                    app:layout_constraintWidth_percent="0.80"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />


                            </androidx.constraintlayout.widget.ConstraintLayout>
                        </androidx.cardview.widget.CardView>

                        <TextView
                            android:id="@+id/textView11"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="12dp"
                            android:layout_marginTop="18dp"
                            android:background="@drawable/gradinat_background"
                            android:elevation="12dp"
                            android:fontFamily="@font/roboto_bold"
                            android:padding="5dp"
                            android:text="city"
                            android:textColor="@color/appMainColor"
                            android:textSize="13sp"
                            app:layout_constraintStart_toStartOf="@+id/con2"
                            app:layout_constraintTop_toTopOf="parent" />


                        <androidx.cardview.widget.CardView
                            android:id="@+id/con2"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="32dp"
                            app:cardCornerRadius="12dp"
                            app:cardElevation="0dp"
                            app:layout_constraintStart_toEndOf="@+id/con"
                            app:layout_constraintTop_toTopOf="parent"


                            app:layout_constraintWidth_percent="0.32">

                            <androidx.constraintlayout.widget.ConstraintLayout

                                android:layout_width="match_parent"
                                android:layout_height="51dp"
                                android:background="@drawable/card_with_border_main2"

                                >


                                <Spinner
                                    android:id="@+id/citySpinner"
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:autofillHints="Drinks Category"
                                    android:contentDescription="@string/app_name"
                                    android:dropDownSelector="@color/appMainColor"
                                    android:overlapAnchor="false"
                                    android:popupBackground="@color/white"
                                    android:spinnerMode="dropdown"
                                    android:visibility="visible"
                                    style="@style/SpinnerTheme"
                                    app:layout_constraintWidth_percent="0.80"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent" />


                            </androidx.constraintlayout.widget.ConstraintLayout>
                        </androidx.cardview.widget.CardView>


                        <TextView
                            android:id="@+id/textView12"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="12dp"
                            android:layout_marginTop="18dp"
                            android:background="@drawable/gradinat_background"
                            android:elevation="12dp"
                            android:fontFamily="@font/roboto_bold"
                            android:padding="5dp"
                            android:text="Percentage"
                            android:textColor="@color/appMainColor"
                            android:textSize="13sp"
                            app:layout_constraintStart_toStartOf="@+id/con3"
                            app:layout_constraintTop_toTopOf="parent" />


                        <androidx.cardview.widget.CardView
                            android:id="@+id/con3"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="32dp"
                            app:cardCornerRadius="10dp"
                            app:cardElevation="0dp"
                            app:layout_constraintStart_toEndOf="@+id/con2"
                            app:layout_constraintTop_toTopOf="parent"


                            app:layout_constraintWidth_percent="0.32">

                            <androidx.constraintlayout.widget.ConstraintLayout

                                android:layout_width="match_parent"
                                android:layout_height="51dp"
                                android:background="@drawable/card_with_border_main2"

                                >


                                <EditText
                                    android:id="@+id/Percentage"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:textSize="13sp"
                                    android:padding="10dp"
                                    android:background="@drawable/back"
                                    android:textColorHint="@color/border"
                                    android:fontFamily="@font/roboto_bold"
                                    android:textColor="@color/appMainColor"
                                    app:layout_constraintBottom_toBottomOf="parent"
                                    app:layout_constraintStart_toStartOf="parent"
                                    app:layout_constraintTop_toTopOf="parent"
                                    app:layout_constraintWidth_percent="0.75"/>


                                <ImageView
                                    android:id="@+id/imageView21"
                                    android:layout_width="0dp"
                                    android:layout_height="match_parent"
                                    android:background="@color/appMainColor"
                                    android:padding="15dp"
                                    android:src="@drawable/ic_percentage"
                                    app:layout_constraintEnd_toEndOf="parent"
                                    app:layout_constraintWidth_percent="0.25"
                                    app:tint="@color/white" />


                            </androidx.constraintlayout.widget.ConstraintLayout>
                        </androidx.cardview.widget.CardView>

                    </androidx.constraintlayout.widget.ConstraintLayout>
                </androidx.cardview.widget.CardView>

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.cardview.widget.CardView>

        <Button
            android:id="@+id/confirm"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:background="@drawable/btn_style"
            android:fontFamily="@font/roboto_bold"
            android:padding="20dp"
            android:text="Confirm"
            android:textColor="#ffffff"
            android:textSize="16sp"
            android:layout_marginTop="20dp"
            app:layout_constraintEnd_toEndOf="@+id/cardView15"
            app:layout_constraintStart_toStartOf="@+id/cardView15"
            app:layout_constraintTop_toBottomOf="@+id/cardView15" />
    </androidx.constraintlayout.widget.ConstraintLayout>
    </androidx.cardview.widget.CardView>


</androidx.constraintlayout.widget.ConstraintLayout>